{
  "name": "node-red-contrib-rainbird",
  "version": "1.0.18",
  "description": "Node-RED nodes for controlling RainBird LNK2 sprinkler controller via local connection. This wraps the RainBird SIP protocol using AES encryption. It uses a slightly modified version of node-rainbird originally developed by @bbreukelen.",
  "main": "index.js",
  "author": {
    "name": "Maciej Szulc",
    "email": "twoj_email@example.com"
  },
  "license": "GPL-3.0-or-later",
  "keywords": [
    "node-red",
    "rainbird",
    "sprinkler",
    "irrigation",
    "lnk2",
    "smart-home"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/maciejs20/node-red-contrib-rainbird.git"
  },
  "bugs": {
    "url": "https://github.com/maciejs20/node-red-contrib-rainbird/issues"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "node-red": {
    "version": ">=1.3.7",
    "nodes": {
      "rainbird-info": "rainbird-info.js",
      "rainbird-state": "rainbird-state.js",
      "rainbird-active": "rainbird-active.js",
      "rainbird-config": "config.js",
      "rainbird-startZone": "rainbird-startZone.js",
      "rainbird-stopZone": "rainbird-stopZone.js",
      "rainbird-delayStart": "rainbird-delayStart.js",
      "rainbird-startProgram": "rainbird-startProgram.js",
      "rainbird-getDelay": "rainbird-getDelay.js"
    }
  },
  "dependencies": {
    "aes-js": "^3.1.2",
    "base64-arraybuffer": "^1.0.1"
  },
  "devDependencies": {
    "jest": "^27.3.1"
  },
  "scripts": {
    "test": "echo \"No test specified\" && exit 0"
  }
}
